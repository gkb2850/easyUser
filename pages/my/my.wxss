.container {
  padding-bottom: 28rpx;
}

.container .user_top .top_bg {
  height: 300rpx;
  background: #31CC90;
}

.container .user_top .top_floor_cont {
  margin: -260rpx 28rpx 0;
}

.container .user_top .top_floor_cont .user_header {
  display: flex;
}

.container .user_top .top_floor_cont .user_header .l_box {
  position: relative;
  height: 120rpx;
  width: 120rpx;
  margin-right: 20rpx;
}

.container .user_top .top_floor_cont .user_header .l_box .user_img {
  height: 100%;
  width: 100%;
  border-radius: 50%;
  box-sizing: border-box;
  border: 2rpx solid #fff;
}

.container .user_top .top_floor_cont .user_header .l_box .user_dj {
  position: absolute;
  right: 0;
  bottom: 0;
  height: 36rpx;
  width: 30rpx;
}

.container .user_top .top_floor_cont .user_header .c_box {
  flex: 1;
}

.container .user_top .top_floor_cont .user_header .c_box .name {
  height: 34rpx;
  line-height: 34rpx;
  margin-top: 22rpx;
  font-size:36rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
}

.container .user_top .top_floor_cont .user_header .c_box .dj_box {
  margin-top: 20rpx;
  display: flex;
  height: 24rpx;
}

.container .user_top .top_floor_cont .user_header .c_box .dj_box>view {
  height: 24rpx;
  line-height: 24rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
}

.container .user_top .top_floor_cont .user_header .c_box .dj_box>view:first-child {
  margin-right: 10rpx;
}

.container .user_top .top_floor_cont .user_header .r_box {
  display: inline-flex;
  height: 28rpx;
  margin-top: 26rpx;
}

.container .user_top .top_floor_cont .user_header .r_box>view {
  height: 28rpx;
  line-height: 28rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
}

.container .user_top .top_floor_cont .chart_box {
  margin-top: 32rpx;
  border-radius: 10rpx;
  overflow: hidden;
  height: 268rpx;
}

.container .user_top .top_floor_cont .chart_box .swiper_box {
  width: 100%;
  height: 100%;
}

.container .user_top .top_floor_cont .chart_box .swiper_box .swiper_item {
  width: 100%;
  height: 100%;
}

.container .user_top .top_floor_cont .chart_box .swiper_box .swiper_item image {
  height: 100%;
  width: 100%;
  border-radius: 10rpx;
  display: block;
}

.container .user_top .top_floor_cont .trem_box {
  padding: 30rpx 0;
  border-bottom: 2rpx solid #EEEEEE;
  height: 40rpx;
  display: flex;
  align-items: center;
}

.container .user_top .top_floor_cont .trem_box .l_img {
  height: 40rpx;
  width: 40rpx;
  margin-right: 20rpx;
}

.container .user_top .top_floor_cont .trem_box view {
  height: 26rpx;
  line-height: 26rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(0,0,0,1);
  flex: 1;
}

.container .user_top .top_floor_cont .trem_box .right_icon {
  height: 24rpx;
  width: 24rpx;
}

.container .user_top .top_floor_cont .repenvelop_box {
  height: 110rpx;
  margin-top: 28rpx;
  position: relative;
}

.container .user_top .top_floor_cont .repenvelop_box>image {
  height: 100%;
  width: 100%;
  display: block;
}

.container .user_top .top_floor_cont .repenvelop_box .hb_box {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  display: flex;
}

.container .user_top .top_floor_cont .repenvelop_box .hb_box>view {
  flex: 1;
  text-align: center;
  height: 100%;
  line-height: 110rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
}

.container .main_box {
  padding: 0 28rpx;
  box-sizing: border-box;
}

.container .main_box .my_yq_box {
  padding: 36rpx 20rpx;
  box-sizing: border-box;
  margin-top: 28rpx;
  box-shadow:0px 4rpx 26rpx 0px rgba(102,102,102,0.1);
  border-radius:10rpx;
}

.container .main_box .my_yq_box .title_txt {
  height: 32rpx;
  line-height: 32rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(0,0,0,1);
}

.container .main_box .my_yq_box .cont_box {
  display: flex;
  align-items: center;
  margin-top: 26rpx;
}

.container .main_box .my_yq_box .cont_box .item_box {
  padding-top: 10rpx;
  flex: 1;
}

.container .main_box .my_yq_box .cont_box .item_box .num {
  text-align: center;
  height: 26rpx;
  line-height: 26rpx;
  font-size:36rpx;
  font-family:DIN;
  font-weight:bold;
  color:rgba(0,0,0,1);
  margin-bottom: 16rpx;
}

.container .main_box .my_yq_box .cont_box .item_box .txt {
  text-align: center;
  height: 20rpx;
  line-height: 20rpx;
  font-size:20rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(102,102,102,1);
}

.container .main_box .my_yq_box .cont_box .line {
  height: 32rpx;
  width: 2rpx;
  background: #EEEEEE;
}

.container .main_box .my_order_box {
  padding: 0 20rpx 36rpx;
  box-sizing: border-box;
  box-shadow:0px 4rpx 26rpx 0px rgba(102,102,102,0.1);
  border-radius:10rpx;
  margin-top: 28rpx;
}

.container .main_box .my_order_box .title_box {
  padding-top: 36rpx;
  display: flex;
  align-items: center;
  height: 32rpx;
  justify-content: space-between;
}

.container .main_box .my_order_box .title_box .l_txt {
  height: 32rpx;
  line-height: 32rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(0,0,0,1);
}

.container .main_box .my_order_box .title_box .r_box {
  display: flex;
  height: 100%;
  align-items: center;
  flex: 1;
  justify-content: flex-end;
}

.container .main_box .my_order_box .title_box .r_box view {
  height: 24rpx;
  line-height: 24rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(102,102,102,1);
  margin-right: 8rpx;
}

.container .main_box .my_order_box .title_box .r_box image {
  width: 16rpx;
  height: 16rpx;
}

.container .main_box .my_order_box .box_icon {
  margin-top: 36rpx;
  display: flex;
}

.container .main_box .my_order_box .box_icon .item_icon {
  flex: 1;
}

.container .main_box .my_order_box .box_icon .item_icon:first-child {
  flex: none;
  padding-right: 60rpx;
}

.container .main_box .my_order_box .box_icon .item_icon:last-child {
  flex: none;
  padding-left: 60rpx;
}

.container .main_box .my_order_box .box_icon .item_icon image {
  height: 54rpx;
  width: 54rpx;
  margin: 0 auto 12rpx;
  display: block;
}

.container .main_box .my_order_box .box_icon .item_icon view {
  height: 20rpx;
  line-height: 20rpx;
  font-size:20rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(0,0,0,1);
  text-align: center;
}

.container .main_box .my_wallet_box {
  margin-top: 28rpx;
  box-shadow:0px 4rpx 26rpx 0px rgba(102,102,102,0.1);
  border-radius:10rpx;
  padding: 36rpx 20rpx;
  box-sizing: border-box;
}

.container .main_box .my_wallet_box .titile_txt {
  height: 32rpx;
  line-height: 32rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(0,0,0,1);
}

.container .main_box .my_wallet_box .cont_box {
  margin-top: 36rpx;
  display: flex;
  flex-wrap: wrap;
}

.container .main_box .my_wallet_box .cont_box .item_txtbox {
  flex: 33.33%;
}

.container .main_box .my_wallet_box .cont_box .item_txtbox:nth-child(1) {
  margin-bottom: 40rpx;
}

.container .main_box .my_wallet_box .cont_box .item_txtbox:nth-child(2) {
  margin-bottom: 40rpx;
}

.container .main_box .my_wallet_box .cont_box .item_txtbox:nth-child(3) {
  margin-bottom: 40rpx;
}

.container .main_box .my_wallet_box .cont_box .item_txtbox .num {
  height: 26rpx;
  line-height: 26rpx;
  font-size:36rpx;
  font-family:DIN;
  font-weight:bold;
  color:rgba(49,204,144,1);
  margin-bottom: 16rpx;
  text-align: center;
}

.container .main_box .my_wallet_box .cont_box .item_txtbox .txt {
  height: 20rpx;
  line-height: 20rpx;
  font-size:20rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(102,102,102,1);
  text-align: center;
}

.container .main_box .my_fw_box {
  padding: 36rpx 20rpx;
  box-sizing: border-box;
  box-shadow:0px 4rpx 26rpx 0px rgba(102,102,102,0.1);
  border-radius:10rpx;
  margin-top: 28rpx;
}

.container .main_box .my_fw_box .title_txt {
  height: 32rpx;
  line-height: 32rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(0,0,0,1);
}

.container .main_box .my_fw_box .cont_boxs {
  margin-top: 36rpx;
  display: flex;
}

.container .main_box .my_fw_box .cont_boxs .term_box {
  width: 20%;
}

.container .main_box .my_fw_box .cont_boxs .term_box:first-child {
  width: auto;
  padding-right: 60rpx;
}

.container .main_box .my_fw_box .cont_boxs .term_box image {
  height: 54rpx;
  width: 54rpx;
  display: block;
  margin: 0 auto 12rpx;
}

.container .main_box .my_fw_box .cont_boxs .term_box view {
  height: 20rpx;
  line-height: 20rpx;
  font-size:20rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(0,0,0,1);
  text-align: center;
}














