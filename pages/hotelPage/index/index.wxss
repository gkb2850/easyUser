  height: 44rpx;
  line-height: 44rpx;
  border-radius: 4rpx;
  padding:  0 10rpx;
}



.container .screen_maskbox {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 30;
}

.container .screen_maskbox .mask_bg {
  height: 100%;
  width: 100%;
  background: rgba(0, 0, 0, 0.4);
}

.container .screen_maskbox .screen_cont {
  position: absolute;
  width: 100%;
  left: 0;
  bottom: 0;
  border-radius: 32rpx 32rpx 0 0;
  overflow: hidden;
  background: #fff;
}

.container .screen_maskbox .screen_cont .title_box {
  height: 88rpx;
  border-bottom: 2rpx solid #EDEDED;
  position: relative;
}

.container .screen_maskbox .screen_cont .title_box view {
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(0,0,0,1);
}

.container .screen_maskbox .screen_cont .title_box image {
  height: 20rpx;
  width: 20rpx;
  border: 32rpx solid transparent;
  position: absolute;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
}

.container .screen_maskbox .screen_cont .main_cont {
  padding:  0 28rpx;
  box-sizing: border-box;
}

.container .screen_maskbox .screen_cont .main_cont .trem_box {
  border-bottom: 2rpx solid #EEEEEE;
  padding: 32rpx 0 8rpx 0; 
}

.container .screen_maskbox .screen_cont .main_cont .trem_box .t_txt {
  height: 26rpx;
  line-height: 26rpx;
  margin-bottom: 20rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(0,0,0,1);
}

.container .screen_maskbox .screen_cont .main_cont .trem_box .cont_txt_box {
  display: flex;
  flex-wrap: wrap;
}

.container .screen_maskbox .screen_cont .main_cont .trem_box .cont_txt_box .item_txt {
  overflow: hidden;
  text-align: center;
  text-overflow: ellipsis;
  white-space: nowrap;
  height: 80rpx;
  line-height: 80rpx;
  background: #EEEEEE;
  border-radius: 10rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  width: 24%;
  padding: 0 6rpx;
  box-sizing: border-box;
  margin-left: 1.33%;
  margin-bottom: 24rpx;
}

.container .screen_maskbox .screen_cont .main_cont .trem_box .cont_txt_box .item_txt.active {
  background: #31CC90;
  color: #fff;
}

.container .screen_maskbox .screen_cont .main_cont .trem_box .cont_txt_box .item_txt:nth-child(4n + 1) {
  margin-left: 0;
}

.container .screen_maskbox .screen_cont .bot_btn_box {
  height: 140rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.container .screen_maskbox .screen_cont .bot_btn_box>view:first-child {
  height: 68rpx;
  line-height: 68rpx;
  border-radius: 34rpx;
  border: 2rpx solid #31CC90;
  box-sizing: border-box;
  text-align: center;
  width: 200rpx;
  margin-right: 56rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(0,0,0,1);
}

.container .screen_maskbox .screen_cont .bot_btn_box>view:last-child {
  height: 68rpx;
  line-height: 68rpx;
  background: #31CC90;
  border-radius: 34rpx;
  box-sizing: border-box;
  text-align: center;
  width: 200rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:#fff;
}







