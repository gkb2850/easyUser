.container {

}

.container .header_box {
  height: 88rpx;
  display: flex;
  justify-content: space-between;
  background: #31CC90;
  padding: 0 28rpx;
  box-sizing: border-box;
}

.container .header_box .userbox {
  height: 100%;
  width: 80rpx;
}

.container .header_box .userbox>image {
  height: 52rpx;
  width: 52rpx;
  margin: 18rpx 0;
  display: block;
  border-radius: 26rpx;
}

.container .header_box .ordericon_box {
  display: inline-flex;
  height: 100%;
}

.container .header_box .ordericon_box .sooneso_icon {
  height: 36rpx;
  width: 36rpx;
  display: block;
  margin: 26rpx 0;
}

.container .header_box .ordericon_box .addIcon_box {
  height: 36rpx;
  width: 36rpx;
  position: relative;
  margin: 26rpx 0 26rpx 40rpx;
  z-index: 20;
}

.container .header_box .ordericon_box .add_icon {
  height: 36rpx;
  width: 36rpx;
  display: block;
}

.container .header_box .ordericon_box .addIcon_box .mask_box {
  position: absolute;
  right: -10rpx;
  top: 50rpx;
  width: 236rpx;
}

.container .header_box .ordericon_box .addIcon_box .mask_box .top_icon {
  height: 20rpx;
  width: 20rpx;
  background: #fff;
  transform: rotateZ(45deg);
  position: absolute;
  right: 20rpx;
  bottom: -10rpx;
}

.container .header_box .ordericon_box .addIcon_box .mask_box .txt_box {
  background: #fff;
  box-shadow:0px 6px 16px 0px rgba(0, 0, 0, 0.4);
  border-radius:10rpx;
  position: absolute;
  right: 0;
  top: 0;
}

.container .header_box .ordericon_box .addIcon_box .mask_box .txt_box .txt_item {
  height: 80rpx;
  display: flex;
  border-bottom: 2rpx solid #EEEEEE;
  padding: 0 25rpx;
  box-sizing: border-box;
}

.container .header_box .ordericon_box .addIcon_box .mask_box .txt_box .txt_item:last-child {
  border: none;
}

.container .header_box .ordericon_box .addIcon_box .mask_box .txt_box .txt_item>image {
  height: 28rpx;
  width: 28rpx;
  display: block;
  margin: 26rpx 23rpx 26rpx 0;
}

.container .header_box .ordericon_box .addIcon_box .mask_box .txt_box .txt_item>view {
  height: 26rpx;
  line-height: 26rpx;
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(0,0,0,1);
  margin: 27rpx 0;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}



.container .search_box {
  height: 88rpx;
  background: #31CC90;
  padding: 0 28rpx;
  box-sizing: border-box;
  overflow: hidden;
}

.container .search_box .searchinset_box {
  width: 100%;
  height: 64rpx;
  margin: 12rpx 0;
  display: flex;
  background: #fff;
  border-radius: 10rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.container .search_box .searchinset_box .search_img {
 height: 28rpx;
 width: 28rpx;
 display: block;
 margin: 18rpx 12rpx 18rpx 0;
}

.container .search_box .searchinset_box>input {
  height: 100%;
  line-height: 64rpx;
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
  flex: 1;
}

.container .search_box .searchinset_box .inputplace {
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
  height: 100%;
  line-height: 64rpx;
}

.container .search_box .searchinset_box .voice_icon {
  height: 32rpx;
  width: 32rpx;
  display: block;
  margin: 16rpx 0;
}

.container .chartImg_box {
  height: 296rpx;
  background: #fff;
  position: relative;
}

.container .chartImg_box .bg_img {
  height: 284rpx;
  width: 100%;
  display: block;
}

.container .chartImg_box .chartPic_box {
  position: absolute;
  left: 50%;
  top: 24rpx;
  width: 694rpx;
  height: 272rpx;
  transform: translateX(-50%);
  border-radius: 10rpx;
  overflow: hidden;
}

.container .chartImg_box .chartPic_box .swiperbox {
  height: 100%;
  width: 100%;
}

.container .chartImg_box .chartPic_box .swiperbox .swiperItem {
  height: 100%;
  width: 100%;
}

.container .chartImg_box .chartPic_box .swiperbox .swiperItem image {
  height: 100%;
  width: 100%;
  display: block;
}

.container .main_box {
  padding: 0 31rpx 0 33rpx;
  box-sizing: border-box;
  background: #fff;
}

.container .main_box .samllLabel_box {
  display: flex;
  flex-wrap: wrap;
  padding-top: 4rpx;
}

.container .main_box .samllLabel_box .item_box {
  width: 20%;
  margin-top: 39rpx;
}

.container .main_box .samllLabel_box .item_box>image {
  width: 108rpx;
  height: 108rpx;
  display: block;
  margin: 0 auto 4rpx;
}

.container .main_box .samllLabel_box .item_box>view {
  height: 20rpx;
  font-size:20rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  text-align: center;
}

.container .main_box .notice_box {
  height: 80rpx;
  margin: 48rpx 0 28rpx 0;
  background: linear-gradient(90deg,rgba(255,239,214,1) 0%,rgba(252,224,184,1) 100%);
  display: flex;
  padding: 0 24rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  overflow: hidden;
}

.container .main_box .notice_box>image {
  height: 40rpx;
  width: 40rpx;
  display: block;
  margin: 20rpx 16rpx 20rpx 0;
}

.container .main_box .notice_box .notice_txt {
  height: 26rpx;
  line-height: 26rpx;
  width: 520rpx;
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(51,51,51,1);
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin: 27rpx 0;
}

.container .main_box .article_box {
  height: 80rpx;
  display: flex;
  background: #31CC90;
  border-radius: 10rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
  justify-content: space-between;
}

.container .main_box .article_box .left {
  flex: 1;
  height: 100%;
  display: flex;
  overflow: hidden;
}

.container .main_box .article_box .left>image {
  height: 56rpx;
  width: 56rpx;
  display: block;
  margin: 12rpx 40rpx 12rpx 0;
  border-radius: 8rpx;
}

.container .main_box .article_box .left>view {
  height: 28rpx;
  line-height: 28rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,255,255,1);
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin: 26rpx 0;
}

.container .main_box .article_box .right {
  height: 100%;
  display: flex;
  overflow: hidden;
}

.container .main_box .article_box .right .txt {
  height: 22rpx;
  line-height: 22rpx;
  font-size:22rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  margin: 29rpx 0;
}

.container .main_box .article_box .right .line {
  height: 14rpx;
  width: 2rpx;
  margin: 33rpx 12rpx;
  background: #fff;
}

.container .select_box {
  margin-top: 43rpx;
}

.container .select_box .title_box {
  padding: 0 24rpx;
  box-sizing: border-box;
  display: flex;
  height: 31rpx;
}

.container .select_box .title_box .first_txt {
  height: 100%;
  line-height: 31rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(0,0,0,1);
  margin-right: 32rpx;
}

.container .select_box .title_box .second_txt {
  height: 26rpx;
  line-height: 26rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(102,102,102,1);
}

.container .select_box .order_box {
  padding:  28rpx 0 0 24rpx;
  box-sizing: border-box;
  overflow: hidden;
}

.container .select_box .order_box .orderall {
  display: inline-flex;
}

.container .select_box .order_box .orderall .order_itme {
  margin-right: 16rpx;
  overflow: hidden;
  width: 184rpx;
  height: 247rpx;
}

.container .select_box .order_box .orderall .order_itme>image {
  height: 184rpx;
  width: 184rpx;
  display: block;
  margin-bottom: 15rpx;
}

.container .select_box .order_box .orderall .order_itme>view {
  line-height: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(0,0,0,1);
}

.container .noticeImg_box {
  padding: 43rpx 24rpx 0 24rpx;
  box-sizing: border-box;
}

.container .noticeImg_box>image {
  width: 100%;
  height: 160rpx;
  border-radius: 10rpx;
  display: block;
  margin-bottom: 28rpx;
}

.container .assembly_box {
  padding: 0 28rpx;
  box-sizing: border-box;
}



